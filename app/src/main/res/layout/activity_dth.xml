<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.uvish.recharge.ETop.MainActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawingCacheQuality="high"
            android:baselineAlignBottom="true"
            android:hapticFeedbackEnabled="true"
            android:src="@drawable/ic_send_black_24dp"

            android:layout_marginRight="26dp"
            android:layout_marginEnd="26dp"
            android:layout_marginBottom="34dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:onClick="send"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        >
        <android.support.design.widget.FloatingActionButton

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26sp"
            android:layout_marginStart="26sp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="34sp"
            android:drawingCacheQuality="high"
            android:hapticFeedbackEnabled="true"
            android:onClick="balance"
            android:src="@drawable/ic_account_balance_wallet_white_24dp"
            android:layout_marginRight="26dp" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="300sp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginLeft="15sp"
        android:layout_marginRight="15sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center">
            <Spinner
                android:layout_width="match_parent"
                android:layout_height="80sp"
                android:id="@+id/spinner1"
                android:spinnerMode="dialog"
                android:textAlignment="center"
                />
            <RelativeLayout
                android:layout_width="150sp"
                android:layout_height="150sp">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/logodth"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="ID :"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="9sp"
                    android:layout_weight="0.7"
                    android:textSize="20sp"
                    android:textColor="#202020"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="ID"
                    android:id="@+id/id"
                    android:inputType="number"
                    android:layout_weight="0.3"
                    android:textAlignment="center"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Amount :"
                    android:layout_weight="0.7"
                    android:textSize="20sp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="9sp"
                    android:textColor="#202020"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Amount"
                    android:id="@+id/amount1"
                    android:inputType="number"
                    android:layout_weight="0.3"
                    android:textAlignment="center"/>

            </LinearLayout>



        </LinearLayout>



    </LinearLayout>





</RelativeLayout>

